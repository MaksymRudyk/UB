{
  "caption": "Desktops",
  "description": "Application desktops",
  "connectionName": "",
  "sqlAlias": "desk",
  "cacheType": "Session",
  "descriptionAttribute": "caption",
  "documentation": "",
  "attributes": [
    {
      "name": "caption",
      "dataType": "String",
      "size": 255,
      "caption": "Desktop name",
      "description": "",
      "allowNull": false,
      "isMultiLang": true
    },
    {
      "name": "code",
      "dataType": "String",
      "size": 50,
      "caption": "Code",
      "documentation": "Unique desktop code. Used for version upgrade",
      "allowNull": false,
      "isUnique": true
    },
    {
      "name": "description",
      "dataType": "String",
      "size": 255,
      "caption": "Desktop description",
      "description": "",
      "isMultiLang": true,
      "allowNull": true
    },
    {
      "name": "iconCls",
      "dataType": "String",
      "size": 255,
      "caption": "Desktop icon",
      "description": "",
      "allowNull": false,
      "defaultValue": "u-icon-desktop"
    },
    {
      "name": "url",
      "dataType": "String",
      "size": 255,
      "caption": "URL",
      "description": "Static server page URL which is displayed in screen centre of selected desktop"
    },
    {
      "name": "isDefault",
      "dataType": "Boolean",
      "caption": "Dy default?",
      "allowNull": false,
      "defaultValue": "0"
    },
    {
      "name": "displayOrder",
      "dataType": "Int",
      "caption": "Order #",
      "description": "Display order",
      "allowNull": false,
      "defaultValue": "1000"
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": true
    },
    "rls": {
      "func": "RLS.allowForAdminOwnerAndAdmTable"
    },
    "multitenancy": {
      "enabled": false
    }
  }
}
