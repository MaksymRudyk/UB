{
  "caption": "Shortcut",
  "description": "Metadata for build navbars",
  "connectionName": "",
  "sqlAlias": "navsh",
  "cacheType": "None",
  "descriptionAttribute": "caption",
  "documentation": "",
  "attributes": [
    {
      "name": "desktopID",
      "dataType": "Entity",
      "associatedEntity": "ubm_desktop",
      "caption": "Desktop",
      "description": "",
      "allowNull": false
    },
    {
      "name": "parentID",
      "dataType": "Entity",
      "associatedEntity": "ubm_navshortcut",
      "caption": "Shortcut folder",
      "description": "",
      "allowNull": true
    },
    {
      "name": "code",
      "dataType": "String",
      "size": 50,
      "caption": "Code",
      "documentation": "Unique shortcut code. Used for version upgrade",
      "allowNull": false,
      "isUnique": true
    },
    {
      "name": "isFolder",
      "dataType": "Boolean",
      "caption": "Is this folder?",
      "allowNull": false,
      "defaultValue": "0"
    },
    {
      "name": "caption",
      "dataType": "String",
      "size": 255,
      "caption": "Shortcut name",
      "allowNull": false,
      "isMultiLang": true
    },
    {
      "name": "cmdCode",
      "dataType": "Text",
      "caption": "Command code"
    },
    {
      "name": "inWindow",
      "dataType": "Boolean",
      "caption": "In new window",
      "description": "Display command result in new window or inside panel",
      "allowNull": false,
      "defaultValue": "0"
    },
    {
      "name": "isCollapsed",
      "dataType": "Boolean",
      "caption": "Collapsed",
      "description": "Show collapsed at the first start",
      "documentation": "It make sense if isFolder = true",
      "allowNull": false,
      "defaultValue": "0"
    },
    {
      "name": "displayOrder",
      "dataType": "Int",
      "caption": "Order #",
      "description": "Display order (in current folder)",
      "allowNull": false,
      "defaultValue": "1000"
    },
    {
      "name": "iconCls",
      "dataType": "String",
      "size": 50,
      "caption": "Icon (CSS class)",
      "allowNull": true
    },
    {
      "name": "description",
      "dataType": "String",
      "size": 256,
      "caption": "Shortcut description",
      "allowNull": true,
      "isMultiLang": true
    },
    {
      "name": "keywords",
      "dataType": "String",
      "size": 512,
      "caption": "Search keywords",
      "allowNull": true,
      "isMultiLang": true
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": true
    },
    "rls": {
      "func": "RLS.allowForAdminOwnerAndAdmTable"
    },
    "tree": {
      "parentAttr": "parentID"
    },
    "multitenancy": {
      "enabled": false
    }
  }
}
