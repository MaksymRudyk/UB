{
  "caption": "Settings",
  "description": "Application settings (aka about:config in firefox)",
  "connectionName": "",
  "sqlAlias": "stngs",
  "cacheType": "SessionEntity",
  "descriptionAttribute": "settingKey",
  "attributes": [
    {
      "name": "settingKey",
      "dataType": "String",
      "size": 150,
      "caption": "Key",
      "description": "Setting key. To prevent key conflicts key name must start with a model code where key is used. `ubs.numcounter.autoRegWithDeletedNumber`",
      "allowNull": false,
      "isUnique": true
    },
    {
      "name": "name",
      "dataType": "String",
      "size": 150,
      "caption": "Name",
      "description": "Setting name",
      "allowNull": false,
      "isMultiLang": true
    },
    {
      "name": "description",
      "dataType": "String",
      "size": 1024,
      "caption": "Description",
      "description": "Description",
      "allowNull": true,
      "isMultiLang": true
    },
    {
      "name": "type",
      "dataType": "String",
      "size": 32,
      "caption": "Type",
      "description": "Value type",
      "allowNull": true
    },
    {
      "name": "settingValue",
      "dataType": "String",
      "size": 2000,
      "caption": "Value",
      "description": "Value",
      "allowNull": true,
      "isMultiLang": true
    },
    {
      "name": "defaultValue",
      "dataType": "String",
      "size": 150,
      "caption": "Default value",
      "description": "Default value (as specified by developer)",
      "allowNull": true,
      "isMultiLang": true
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": true,
      "safeDelete": false
    }
  }
}