{
  "caption": "Security Audit",
  "description": "Security audit",
  "connectionName": "",
  "sqlAlias": "saud",
  "cacheType": "None",
  "descriptionAttribute": "entity",
  "documentation": "All changes to UBA model entities (except uba_als & uba_subject) + user login related event are logged here",
  "attributes": [
    {
      "name": "entity",
      "dataType": "String",
      "size": 32,
      "caption": "Entity code",
      "allowNull": false
    },
    {
      "name": "entityinfo_id",
      "dataType": "ID",
      "caption": "Instance ID",
      "allowNull": false
    },
    {
      "name": "actionType",
      "dataType": "Enum",
      "enumGroup": "AUDIT_ACTION",
      "caption": "Action",
      "allowNull": false
    },
    {
      "name": "actionUser",
      "dataType": "String",
      "size": 128,
      "caption": "User",
      "description": "User, who perform the action",
      "allowNull": false
    },
    {
      "name": "actionTime",
      "dataType": "DateTime",
      "caption": "Action time",
      "allowNull": false
    },
    {
      "name": "remoteIP",
      "dataType": "String",
      "size": 40,
      "caption": "Remote IP",
      "description": "Caller remote IP address. NULL in case of localhost"
    },
    {
      "name": "targetUser",
      "dataType": "String",
      "size": 128,
      "caption": "Target user",
      "description": "The user name for which the data has changed",
      "allowNull": true
    },
    {
      "name": "targetGroup",
      "dataType": "String",
      "size": 128,
      "caption": "Target group",
      "description": "The group name for which the data has changed",
      "allowNull": true
    },
    {
      "name": "targetRole",
      "dataType": "String",
      "size": 128,
      "caption": "Target role",
      "description": "The role name for which the data has changed",
      "allowNull": true
    },
    {
      "name": "fromValue",
      "dataType": "Text",
      "caption": "Old values",
      "allowNull": true
    },
    {
      "name": "toValue",
      "dataType": "Text",
      "caption": "New values",
      "allowNull": true
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": false,
      "safeDelete": false
    },
    "audit": {
      "enabled": false
    }
  },
  "dbExtensions": {
    "idx_saud_targetUser": {
      "type": "INDEX",
      "definition": {
        "keys": {
          "targetUser": {}
        }
      }
    }
  }
}