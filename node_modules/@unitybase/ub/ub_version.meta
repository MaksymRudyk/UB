{
  "caption": "Models Versions",
  "description": "Current application models versions. Used by migration hooks to perform custom actions. Updated by migration script after migration is success",
  "connectionName": "",
  "descriptionAttribute": "modelName",
  "attributes": [
    {
      "name": "modelName",
      "dataType": "String",
      "caption": "Model",
      "description": "Model code",
      "allowNull": false,
      "size": 32
    },
    {
      "name": "version",
      "dataType": "String",
      "caption": "Version",
      "description": "model version as a string in format XXXYYYZZZ where: XXX-major, YYY-minor ZZZ-patch. All parts are padded by 0 to be a three letter; Example: 2.15.1 = 002015001",
      "allowNull": false,
      "size": 12
    },
    {
      "name": "appliedAt",
      "dataType": "DateTime",
      "caption": "Applied at",
      "defaultValue": "currentDate",
      "allowNull": false
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": false,
      "safeDelete": false
    },
    "multitenancy": {
      "enabled": false
    }
  }
}