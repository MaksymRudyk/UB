{
  "caption": "Applied Migrations",
  "description": "Migration scripts what applied to the current application",
  "connectionName": "",
  "descriptionAttribute": "filePath",
  "attributes": [
    {
      "name": "modelName",
      "dataType": "String",
      "caption": "Model",
      "description": "Model code",
      "allowNull": false,
      "size": 32
    },
    {
      "name": "filePath",
      "dataType": "String",
      "caption": "File path",
      "description": "file path (relative to model _migrate folder)",
      "allowNull": false,
      "size": 256
    },
    {
      "name": "fileSha",
      "dataType": "String",
      "size": 64,
      "caption": "SHA256 of file",
      "allowNull": false
    },
    {
      "name": "appliedAt",
      "dataType": "DateTime",
      "caption": "Applied at",
      "defaultValue": "currentDate",
      "allowNull": false
    }
  ],
  "mixins": {
    "mStorage": {
      "simpleAudit": false,
      "safeDelete": false
    },
    "multitenancy": {
      "enabled": false
    }
  }
}